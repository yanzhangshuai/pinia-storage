{
  "name": "@mwjz/pinia-storage",
  "version": "0.0.2",
  "description": "Persistence state in your pinia üçç store!",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "unpkg": "dist/index.global.js",
  "jsdelivr": "dist/index.global.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "license": "MIT ",
  "author": {
    "name": "mowenjinzhao",
    "email": "yanzhangshuai@126.com"
  },
  "files": [
    "dist/*.js",
    "dist/*.mjs",
    "dist/*.d.ts",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "pinia",
    "plugin",
    "storage",
    "example",
    "starter",
    "boilerplate"
  ],
  "scripts": {
    "build": "        pnpm build:esm & pnpm build:iife",
    "build:esm": "    tsup src/index.ts --format esm,cjs --dts --target es2020",
    "build:iife": "   tsup src/index.ts --format iife --minify --global-name PiniaStorage --target es2020",
    "test": "         jest --passWith-no-tests --watchAll",
    "test:report": "  jest --passWith-no-tests --coverage"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.26",
    "jest": "^27.5.1",
    "pinia": "^2.0.13",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsup": "^5.12.6",
    "typescript": "^4.6.3",
    "vue": "^3.2.33"
  },
  "peerDependencies": {
    "pinia": "^2.0.0"
  }
}
